<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="705.0" style="-fx-background-color: #E3A699;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxloginn.InspectAuditionController">
   <children>
      <Pane layoutX="14.0" layoutY="32.0" prefHeight="354.0" prefWidth="200.0" style="-fx-background-color: #D59D91;">
         <children>
            <VBox prefHeight="383.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="auditionImage" fitHeight="170.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets bottom="5.0" top="15.0" />
                     </VBox.margin>
                  </ImageView>
                  <Label fx:id="auditionName" alignment="CENTER" prefHeight="38.0" prefWidth="200.0" text="Nom Audition" textFill="WHITE">
                     <font>
                        <Font name="System Italic" size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <Button fx:id="btnAccept" mnemonicParsing="false" onAction="#on_Click_btnAccept" prefHeight="25.0" prefWidth="178.0" style="-fx-background-color: GREEN;" text="Accept" textFill="WHITE">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" top="10.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnDecline" mnemonicParsing="false" onAction="#on_Click_btnDecline" prefHeight="25.0" prefWidth="179.0" style="-fx-background-color: red;" text="Decline" textFill="WHITE">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" top="5.0" />
                     </VBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </Pane>
      <MediaView fx:id="auditionVideo" fitHeight="230.0" fitWidth="450.0" layoutX="238.0" layoutY="74.0">
         <viewport>
            <Rectangle2D />
         </viewport></MediaView>
      <Button fx:id="btnPlay" layoutX="404.0" layoutY="318.0" mnemonicParsing="false" onAction="#on_Click_btnPlay" prefHeight="25.0" prefWidth="47.0" style="-fx-background-color: #B6877D;" text="Play" />
      <Button fx:id="btnVideo" layoutX="463.0" layoutY="318.0" mnemonicParsing="false" onAction="#on_Click_btnPause" style="-fx-background-color: #B6877D;" text="Pause" />
      <Label fx:id="userName" layoutX="232.0" layoutY="32.0" text="Audition Inspection" textFill="#666261">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnBack" layoutX="641.0" layoutY="361.0" mnemonicParsing="false" onAction="#on_Click_btnBack" text="Back" />
   </children>
</AnchorPane>
